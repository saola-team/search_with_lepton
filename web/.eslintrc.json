{
  "plugins": [
    "unused-imports"
  ],
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "unused-imports/no-unused-imports": "error",
    "indent": ["error", 2],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}